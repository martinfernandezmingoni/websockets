
<h1>Listado de productos</h1>
<ul>
  {{#each products}}
  <li>
    {{#each this}}
    <div class="property">
      <strong>{{@key}}:</strong> 
      <span class="span">{{this}}</span>
    </div>
    {{/each}}
  </li>
  {{/each}}
</ul>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>
